{
  "name": "project",
  "private": true,
  "version": "1.0.0",
  "description": "A full-stack project with backend, admin, and frontend modules",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:backend": "node index.js",
    "dev:frontend": "vite --config ./frontend/vite.config.js",
    "dev:admin": "vite --config ./admin/vite.config.js",
    "build:frontend": "vite build --config ./frontend/vite.config.js",
    "build:admin": "vite build --config ./admin/vite.config.js",
    "start:frontend": "vite preview --config ./frontend/vite.config.js",
    "start:admin": "vite preview --config ./admin/vite.config.js",
    "lint": "eslint .",
    "build": "npm run build:frontend && npm run build:admin"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.3",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "vite": "^5.4.1"
  }
